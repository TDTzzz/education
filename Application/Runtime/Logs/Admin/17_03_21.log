SQL: INSERT INTO `article1` (`title`,`author`,`resource`,`type`,`content`,`department`,`time`) VALUES ('信息科学与技术学部简介','教务处','文华教务网','专业特色','&lt;p&gt;&lt;span style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; float: none; display: inline !important; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;信息科学与技术学部&lt;/span&gt;&lt;br style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;/&gt;&lt;span style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; float: none; display: inline !important; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp; （2014年）&lt;/span&gt;&lt;br style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;/&gt;&lt;span style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; float: none; display: inline !important; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; 信息科学与技术学部成立于2003年，学部现设通信工程、电子信息工程、光电信息工程、电子科学与技术、软件工程、计算机科学与技术、自动化等7个本科专 业，及计算机应用技术1个专科专业。已毕业本科生近4000人，专科生近700人，在校学生近2650人。&lt;/span&gt;&lt;br style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;/&gt;&lt;span style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; float: none; display: inline !important; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;学部设电子信息工程系、光信息科学与技术系、电子科学与技术系、计算机科学与技术系、控制科学与技术系和电路与电子技术基础系，及学部教学实验中心。实验 中心下设计算机科学与技术、电子信息技术、通信技术&amp;nbsp;(SDH)、智能与自动化系统集成(GE)、光电信息技术、微电子技术及电子技术等七个实验室，及电 工电子、创新与实训等二个实践教学基地，实验中心面积1.5万平方米，设备总资产4000多万元。&lt;/span&gt;&lt;br style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;/&gt;&lt;span style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; float: none; display: inline !important; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; 2012年，信息与通信工程学科获准为“十二五”湖北省重点（培育）学科。通信工程专业为湖北省独立学院重点培育本科专业立项建设项目、湖北省普通高等学 校战略性新兴（支柱）产业人才培养计划本科项目，以及湖北省普通本科高校专业综合改革试点项目；信息技术科研基地为校级科研基地；软件工程、自动化专业为 文华学院特色专业建设项目。&lt;/span&gt;&lt;br style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;/&gt;&lt;span style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; float: none; display: inline !important; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;学部现有教职工102人，其中教授&amp;nbsp;9人（享受国务院政府特殊津贴的教授3人、获国家教学名师称号的教授1人），副教授（含高工）29人，具有博士学位教 师9人。近年来教师承接或完成科研与教改项目共77个，其中国家自然科学基金项目1项，湖北省自然科学基金项目3项。发表研究论文140多篇。出版教材 70余本，获专利12项。&lt;/span&gt;&lt;br style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;/&gt;&lt;span style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; float: none; display: inline !important; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&amp;nbsp;学部教师60人次获学院各类教学类奖励，5人获院三育人奖，&amp;nbsp;2人获伯乐奖，1人获学院科研特别奖。学部辅导员在学院级“优秀思政工作者”评比中累计获奖14人次，其中“辅导员标兵”1人，“十佳辅导员”2人；1人获“湖北省优秀思想政治教育工作者”称号。&lt;/span&gt;&lt;br style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;/&gt;&lt;span style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; float: none; display: inline !important; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; 学部重视学生个性化教育和创新实践能力的培养，先后与武汉烽火集团等企业共建了23个校外生产实习基地和4个校内学生潜能培育平台。学部现有湖北省大学生 创新创业训练项目2个，获国家及国、内外知名企业各类技能证书近千人次。近3年学生参加全国各类大学生学科竞赛获国家及省级奖励298人次。学部考研录取 率连年攀升，近两年平均考研率为19%。2012届光信息科学与技术专业考研录取率为35.84%，2013届电子科学与技术专业考研录取率为 37.25%，均创学院历年专业考研率最好成绩，其中近50%学生为华中科技大学、武汉大学、武汉理工大学等985、211高校录取。学部各专业毕业生就 业率在90%以上，电子信息工程专业2008届学生李宇迪，毕业后凭借在校期间获得的国家专利，创办武汉迪奥尔照明有限公司，2012年5月被评选为“湖 北省大学生创业十佳之星”。&lt;/span&gt;&lt;img title=&quot;1490091080291755.jpg&quot; alt=&quot;21620270762900.jpg&quot; src=&quot;/ueditor/php/upload/image/20170321/1490091080291755.jpg&quot;/&gt;&lt;span style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; float: none; display: inline !important; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;2009级电子科学与技术专业考研学生合影&lt;img title=&quot;1490091091125050.jpg&quot; alt=&quot;2172752401301.jpg&quot; src=&quot;/ueditor/php/upload/image/20170321/1490091091125050.jpg&quot;/&gt;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; float: none; display: inline !important; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;毕业生李宇迪被评为“湖北省大学生创业十佳之星”&lt;/span&gt;&lt;img title=&quot;正在上传...&quot; class=&quot;loadingclass&quot; id=&quot;loading_j0jgafed&quot; src=&quot;http://58.49.5.110/education/Public/ueditor/themes/default/images/spacer.gif&quot;/&gt;&lt;/p&gt;','信息科学与技术学部','1490091109') [ RunTime:0.0077s ]
