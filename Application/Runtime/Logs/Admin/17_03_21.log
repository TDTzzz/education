SQL: INSERT INTO `article1` (`title`,`author`,`resource`,`type`,`content`,`department`,`time`) VALUES ('机电学部简介','教务处','文华教务处','专业特色','&lt;p style=&quot;margin: 0px; padding: 0px; border: 0px currentColor; border-image: none; color: rgb(0, 0, 0); text-transform: none; text-indent: 2em; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;text-align: justify; line-height: 24px; font-family: 宋体;&quot;&gt;机械与电气工程学部由机械系、电气系、能源系、船舶系、材料系五个系及制图教研室和实验中心、工程实训中心组成。现有&lt;/span&gt;机械设计制造及其自动化、电气工程及其自动化、能源与动力工程、船舶与海洋工程、材料成型及控制工程&lt;span style=&quot;text-align: justify; line-height: 24px; text-indent: 2em; font-family: 宋体;&quot;&gt;五 个本科专业学生2000余人，机械设计与制造、应用电子技术两个专科专业学生600余人。 &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;br/&gt;&lt;/p&gt;&lt;p style=&quot;margin: 0px; padding: 0px; border: 0px currentColor; border-image: none; color: rgb(0, 0, 0); text-transform: none; text-indent: 2em; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;text-align: justify; line-height: 24px; font-family: 宋体;&quot;&gt;学部实验中心下设机械基础实验室、热工基础实验室、材料成型与性能检测实验室、&lt;/span&gt;&lt;span style=&quot;text-align: justify; line-height: 24px; font-family: 宋体;&quot;&gt;船舶计算机设计与制造实验室、电力系统继电保护与综合控制实验室&lt;/span&gt;&lt;span style=&quot;text-align: justify; line-height: 24px; font-family: 宋体;&quot;&gt;等 14个实验室。工程实训中心面积4000多平方米，设备340台套，固定资产达千万，实训工种21个。学部现有教职工83人，其中国家教学名师1人，教授 6人，副教授11人，近年来教师发表论文60余篇，出版教材（专著）20余部，获湖北省自然科学基金项目1项，国家专利8项。&lt;/span&gt;&lt;span style=&quot;text-align: justify; line-height: 24px; font-family: 宋体;&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;br/&gt;&lt;br style=&quot;text-align: justify; line-height: 24px; font-family: 宋体;&quot;/&gt;&lt;span style=&quot;text-align: justify; line-height: 24px; font-family: 宋体;&quot;&gt;&amp;nbsp; &amp;nbsp;学部重视学生创新精神与实践能力的培养，注重学生综合素质与能力的提升，先后与东风汽车有限公司等19个企业共建学生生产实习基地。在学院“以学生为 本”的办学理念与学院个性化教育办学特色思路的指导下，学部提出了“强专业、重实践、个性化”的学生培养思路，在强化第一课堂教学的同时，组建了学生机械 创新工作室、张帆创新工作室、节能减排创新工作室、材料成型创新工作室、电气创新设计工作室、船舶创新设计工作室等学生第二课堂创新实践平台；指导学生创 办科技社团，积极组织开展各类科技文化活动，学部课外科技活动丰富多彩，在老师精心指导和鼓励下，学生努力开展创新实践和积极参加全国、全省各类学科竞赛 活动，人才培养取得骄人成绩。学部现有湖北省大学生创新创业训练项目3个，学生在全国大学生学科竞赛中获得一等奖12项，二等奖53项，获国家专利17 项。毕业设计贴近工程实际，获湖北省优秀学士论文26篇。学部考研录取率连续三年达20%，近50%在华中科技大学、武汉大学和武汉理工大学等985或 211高校深造。机械设计制造及其自动化专业2012届毕业生张帆，在校期间完成创新实践课题26项，拥有4项实用新型设计专利，先后被评为湖北省大学生 年度人物（全省共10人），湖北省优秀大学生。学部毕业生就业率保持在95%以上，机械制造行业、电力系统、信息产业、航天工程、海洋工程都有我们的学生 在努力工作。&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;img title=&quot;1490090972124004.jpg&quot; src=&quot;/ueditor/php/upload/image/20170321/1490090972124004.jpg&quot;/&gt;&lt;/p&gt;&lt;p style=&quot;margin: 0px; padding: 0px; border: 0px currentColor; border-image: none; text-align: center; color: rgb(0, 0, 0); text-transform: none; text-indent: 2em; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;line-height: 28px; font-size: 14px;&quot;&gt;机电学部学生在全国大学生学科竞赛中获得一等奖12项，二等奖53项&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体;&quot;&gt;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;br class=&quot;Apple-interchange-newline&quot;/&gt;&lt;/p&gt;&lt;p&gt;&lt;img title=&quot;1490090975481912.jpg&quot; src=&quot;/ueditor/php/upload/image/20170321/1490090975481912.jpg&quot;/&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;text-align: right; color: rgb(0, 0, 0); text-transform: none; text-indent: 28px; letter-spacing: normal; font-family: 宋体; font-size: 14px; font-style: normal; font-weight: normal; word-spacing: 0px; float: none; display: inline !important; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;机电学部2013届117位录取为研究生的学生与学院领导老师合影&lt;span class=&quot;Apple-converted-space&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;/p&gt;','机械与电气工程学部','1490090994') [ RunTime:0.0056s ]
