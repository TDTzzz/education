SQL: INSERT INTO `article1` (`title`,`author`,`resource`,`type`,`content`,`time`) VALUES ('关于做好2015级本科学生修读双学位报名工作的通知','教务处','文华教务网','教务通知','&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;各学部、2015级本科学生：&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 28px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;为培养复合型应用人才，适应我校个性化教育发展的需要，学校决定在原有辅修制的基础上，实行双学位教育。根据《文华学院辅修专业、双学位教育管理规定》（&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;院教〔2014〕8号&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;），学校将于2016-2017学年度面向2015级本科学生开设《工商管理》专业双学士学位班。现将报名的有关事项和教学安排通知如下：&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 19px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;一、报名资格：&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 28px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;1&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;、&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;表现良好，学有余力的2015级在校普通全日制本科学生；&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 28px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;2&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;、&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;主修专业必修课程考试成绩优良；&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;有两门以上（含2门）课程成绩不及格者，不得修读双学位；&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 28px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;3&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;、&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;无违纪处分记录者；&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 28px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;4&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;、&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;申请修读双学士学位专业必须跨学科申报（即管理学学科专业本次不可申请）。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 19px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;二、《工商管理》开设主要课程：&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 28px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;管理学、管理经济学、管理统计学、管理信息系统、基础会计、财务管理、生产运作管理、市场营销、经济法、人力资源管理、企业战略管理。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 19px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;三、《工商管理》专业双学士学位班学分与学位授予：&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 19px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;双学位专业课程学习结束时间与学生主修专业学习完成时间一致。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 28px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;修满规定课程达50学分或以上，通过论文答辩或毕业设计，并获得主修专业毕业证、学位证的非管理学学科的学生，可获得学校颁发的管理学学士学位证书；修满规定课程达25学分或以上，且符合学院关于辅修专业要求的学生，可获得学校颁发的辅修专业证书。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 19px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;四、报名相关事宜：&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 28px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;1&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;、报名时间2016年12月20日-2016年12月29日&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 28px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;2&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;、报名方式：&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; 1&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;）申请学生主修课程都合格者，直接进入教务系统网上报名，操作步骤：学生登陆学院教务管理系统（网址为：http://jw1.hustwenhua.net和http://jw2.hustwenhua.net两个地址），进入系统之后，请在主页面上方点选“信息维护”中的“辅修报名”项；打开的页面中在申请专业选择“工商管理”，然后点击下方“报名”按钮即可。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; 2&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;）申请学生主修课程如有一门不合格者，需在报名时间内填写《文华学院本科双学位专业申请表》报学部教务办审核通过后报教务处备案才可参加学习。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 28px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;3&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;、资格审查：教务处将根据学生报名信息进行资格审查，审核通过的名单将予以公示，公示期为一周。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 28px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;4&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;、缴费、听课证领取：&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 28px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;经录取的学生&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;于2017年2月20日-2月22日持本人学生证到学校财务处（行政楼B-120收费中心）办理缴费手续。修读双学位培养费分二次缴纳。本次缴纳25学分的培养费为：260元×25学分=6500元。学生持缴费收据到教务处（行政楼A102）领取&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 20px;&quot;&gt;“&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;听课证&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 20px;&quot;&gt;”&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 20px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 20px;&quot;&gt;五、教学安排及其他注意事项：&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 30px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 20px;&quot;&gt;1&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;、学生凭“听课证”，按课表到学校指定地点上课。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 28px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;2&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;、上课时间一般为双休日。各学期具体开课时间、地点等教学安排见个人课表，学期初学生可在教务系统中查询；如有变动另行通知。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 28px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-stroke-width: 0px;&quot;&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;3&lt;/span&gt;&lt;span style=&quot;font-family: 宋体; font-size: 19px;&quot;&gt;、学生必须在报名规定时间内履行申请手续，逾期不予受理。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;color: rgb(0, 0, 0); text-transform: none; line-height: 32px; text-indent: 0px; letter-spacing: normal; font-family: Simsun; font-size: 16px; font-style: normal; font-weight: normal; margin-bottom: 1em; word-spacing: 0px; white-space: normal; orphans: 2; widows: 2; background-color: rgb(255, 255, 255); font-variant-ligatures: normal; font-variant-caps: normal; -webkit-text-