SQL: INSERT INTO `article1` (`title`,`author`,`resource`,`type`,`content`,`time`) VALUES ('关于汇总学部实习实训基地的通知','文华教务处','教务网','质量工程','&lt;p style=&quot;margin-bottom: 1em; font-family: Simsun; white-space: normal; background-color: rgb(255, 255, 255); text-align: center;&quot;&gt;通&amp;nbsp;&amp;nbsp; 知&lt;/p&gt;&lt;p style=&quot;margin-bottom: 1em; font-family: Simsun; white-space: normal; background-color: rgb(255, 255, 255); text-align: center;&quot;&gt;&amp;nbsp;&lt;/p&gt;&lt;p style=&quot;margin-bottom: 1em; font-family: Simsun; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;各有关学部：&lt;/p&gt;&lt;p style=&quot;margin-bottom: 1em; font-family: Simsun; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp;&amp;nbsp; &amp;nbsp;为做好转设评估检查工作，学院重新汇总各专业校外实习实训教学基地所签协议书，请各学部对现有的专业校外实习实训教学基地进行梳理、补充，对已到时限的 协议应及时续签。各学部应注意协议书中存在的一些问题，如协议书不规范、缺协议方的签字、盖章等等，请学部在新签协议书时注意纠正以上问题。&lt;/p&gt;&lt;p style=&quot;margin-bottom: 1em; font-family: Simsun; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp;&amp;nbsp; &amp;nbsp;请学部于11月16日将校外实习实训教学基地协议书的复印件及汇总表（发电子版）交教务处。&lt;/p&gt;&lt;p style=&quot;margin-bottom: 1em; font-family: Simsun; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;/p&gt;&lt;p style=&quot;margin-bottom: 1em; font-family: Simsun; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;教务处&lt;/p&gt;&lt;p style=&quot;margin-bottom: 1em; font-family: Simsun; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;2012年11月7日&lt;/p&gt;&lt;p style=&quot;margin-bottom: 1em; font-family: Simsun; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp;&lt;/p&gt;&lt;p style=&quot;margin-bottom: 1em; font-family: Simsun; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp;&lt;/p&gt;&lt;p style=&quot;margin-bottom: 1em; font-family: Simsun; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;附：1、学部校外实习实训教学基地汇总表（样表）&lt;/p&gt;&lt;p style=&quot;margin-bottom: 1em; font-family: Simsun; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;2、现有各学部校外实习实训教学基地协议书情况&lt;/p&gt;&lt;p style=&quot;margin-bottom: 1em; font-family: Simsun; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&lt;br/&gt;&lt;/p&gt;&lt;p style=&quot;margin-bottom: 1em; font-family: Simsun; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&lt;a href=&quot;http://jwweb.hustwenhua.net/upload/file/20141030/171125958693016.doc&quot; style=&quot;text-decoration: none; color: rgb(51, 51, 51);&quot;&gt;附件：关于汇总学部实习实训基地的通知.doc&lt;/a&gt;&lt;/p&gt;&lt;p&gt;&lt;br/&gt;&lt;/p&gt;','1489398568') [ RunTime:0.0004s ]
